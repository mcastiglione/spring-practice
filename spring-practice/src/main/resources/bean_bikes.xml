<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans.xsd
                        http://www.springframework.org/schema/context
                        http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="rentBikeHour" class="com.javapractice.model.bike.RentBikeHour" scope="prototype">
        <property name="costPerTimeUnit" value="${bike.hour}"/>
    </bean>
    <bean id="rentBikeDay"  class="com.javapractice.model.bike.RentBikeDay" scope="prototype">
        <property name="costPerTimeUnit" value="${bike.day}"/>
    </bean>
    <bean id="rentBikeWeek" class="com.javapractice.model.bike.RentBikeWeek" scope="prototype">
        <property name="costPerTimeUnit" value="${bike.week}"/>
    </bean>
    <bean id="bikeParams"   class="com.javapractice.model.bike.BikeParams" scope="prototype"></bean>

</beans>